{
  "expo": {
    "scheme": "creandolazos",
    "name": "CreandoLazos",
    "slug": "CreandoLazos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2079b4"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.smallkeloft.CreandoLazos"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6675065c-c34e-4151-b998-ee9b96640d41"
      }
    },
    "owner": "smallkeloft",
    "plugins": [
      "expo-system-ui",
      "expo-router"
    ]
  }
}
